<% for(var i=0; i<users.length; i++) { %>
    <tr>
        <td><%= i+1 %></td>
        <td><a ><%= users[i].name %></a></td>
        <td><%= users[i].email %></td>                        
        <td><%= users[i].gender %></td>
        <td><!--<span class="status text-success">&bull;</span>--> <%= users[i].status %></td>
        <td>
            <a href="/update-user?id=<%= users[i]._id %>" class="edit" title="Edit" data-toggle="tooltip"><i class="material-icons">&#xE3C9;</i></a>
            <a href="#" class="delete" title="Delete" data-toggle="tooltip" data-id=<%= users[i]._id %>><i class="material-icons">&#xE5C9;</i></a>
        </td>
    </tr>
<% } %>
